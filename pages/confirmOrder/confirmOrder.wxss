/* 表单模板样式 */
/* 商品动态表单 */
.form-box{
    background: white;
    margin-top: 10rpx;
}

.form-item{
    width: 100%;
    box-sizing: border-box;
    display: flex;
    font-size: 24rpx;
    align-items: center;
    border-bottom: 1rpx solid #ebebeb;
}
.form-item:last-child{
    border: none;
}

.form-item>.item-title-input{
    color: #666666;
    font-size: 24rpx;
    padding: 0 30rpx;
    line-height: 110rpx;
    
}
.form-item >input{
    flex: 1;
    min-height: 100rpx;
    line-height: 100rpx;
    font-size: 28rpx;
    color: #333333;
    box-sizing: border-box;
    padding: 0 20rpx;
     
}
.form-item >textarea{
    flex: 1;
    height: 200rpx;
    line-height: 32rpx;
    font-size: 28rpx;
    color: #333333;
    box-sizing: border-box;
    padding: 20rpx;
     
}
.form-item >picker{
    flex: 1;
    box-sizing: border-box;
    line-height: 110rpx;
    font-size: 28rpx;
    color: #333333;
    box-sizing: border-box;
    padding: 0 20rpx;
}
.form-item radio-group,.form-item checkbox-group{
    flex: 1;
    box-sizing: border-box;
    box-sizing: border-box;
    padding: 0 20rpx;
    padding-bottom: 20rpx;
}
.form-item radio-group label,.form-item checkbox-group label{
    display: block;
    padding-top: 20rpx;
    color: #999999;
    
}
.credit-pay {
    width: 100%;
    height: 98rpx;
    background: #FF9B1F;
    color: #fff;
    text-align: center;
    line-height: 98rpx;
    font-size: 30rpx;
}