/*//分享模态框*/

.share-diy-modal {
    position: fixed;
    top: 0;
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
    align-items: center;
    z-index: 150;
}

.share-diy-enevt {
    width: 100%;
    background: linear-gradient(rgba(255, 255, 255, 0), rgba(200, 200, 200, 0.7));
    padding: 30rpx 0;
}

.share-diy-modal-up {
    animation: showShareUp 200ms;
}

.share-diy-modal-down {
    animation: showShareDown 200ms;
}

@keyframes showShareUp {
    from {
        transform: translateY(100rpx);
    }

    to {
        transform: translateY(0);
    }
}

@keyframes showShareDown {
    from {
        transform: translateY(0);
    }

    to {
        transform: translateY(200rpx);
    }
}

.share-item-box {
    background: white;
    width: 690rpx;
