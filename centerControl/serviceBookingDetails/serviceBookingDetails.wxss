.container{
    background: #fff;
  }
  .tags {
    /* padding-left:120rpx;
    padding-bottom: 12rpx; */
    display: flex;
    flex-wrap: wrap;
    box-sizing: border-box;
  }
  .tag {
    height: 35rpx;
    line-height: 35rpx;
    padding: 0 10rpx;
    position: relative;
    margin-right: 14rpx;
    margin-bottom: 14rpx;
    border-radius: 2rpx;
    color: #fff;
  }
  .container>view{
    padding: 42rpx 30rpx;
    border-bottom: 1rpx solid #f5f5f5;
  }
  
  .container>view>text:first-of-type{
    margin-right: 40rpx;
  }
  