.great {
    animation: great 0.5s;
}

.isLike {
    position: relative;
}

.isLike::after {
    content: "+1";
    font-size: 30rpx;
    font-weight: bold;
    position: absolute;
    white-space: nowrap;
    top: -30rpx;
    left: 50%;
    opacity: 0;
    transform: translate(-50%, 0);
    animation: jia1 0.5s;
}

@keyframes great {
    0% {
        transform: scale(0.6, 0.6);
    }
    20% {
        transform: scale(1.4, 1.4);
    }
    40% {
        transform: scale(0.8, 0.8);
    }
    70% {
        transform: scale(1.1, 1.1);
    }
    100% {
        transform: scale(1, 1);
    }
}

@keyframes jia1 {
    from {
        opacity: 1;
        transform: translate(-50%, 0);
    }
    to {
        opacity: 0;
        transform: translate(-50%, -50rpx);
    }
}


