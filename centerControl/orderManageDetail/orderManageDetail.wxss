.dialog {
    width: 690rpx;
    box-sizing: border-box;
    padding: 0 30rpx;
    transition: .3s;
}

.dialog-this-title {
    line-height: 100%;
    padding-top: 48rpx;
    padding-bottom: 24rpx;
    text-align: center;
}

.dialog-this-label {
    margin-bottom: 20rpx
}

.dialog-this-input {
    height: 80rpx;
    width: 100%;
    border: 1rpx solid #d9d9d9;
    border-radius: 4rpx;
    box-sizing: border-box;
    padding-left: 30rpx;
    position: relative;
    background-color: #f7f7f7;
    margin-bottom: 30rpx;
}

.dialog-this-picker::after {
    content: "\e67c";
    position: absolute;
    font-size: 18rpx;
    color: #999;
    line-height: 100%;
    right: 30rpx;
    top: 50%;
    transform: translateY(-50%);
}

.dialog-this-input>input {
    width: 100%;
}

.dialog-this-refund {
    margin-bottom: 30rpx;
}

.getQRCode {
    width: 100%;
    height: 80rpx;
    box-sizing: border-box;
    border: 1rpx solid #ff9b1f;
    border-radius: 4rpx;
    padding: 0 30rpx;
    margin-top: 20rpx;
    color: #ff9b1f;
    background-color: #fff8f0;
    margin-bottom: 50rpx;
}

.getQRCode>.iconfont {
    margin-right: 18rpx;
}

.dialog-this-submit {
    height: 76rpx;
    width: 382rpx;
    border-radius: 6rpx;
    background-color: #ff9b1f;
    margin: 0 auto 50rpx auto;
}

