.container{
  padding-bottom: 200rpx;
  background-color: #F7F7F7;
}
.mask {
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    position: absolute;
    z-index: 999;
}
.mask-box {
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    position: absolute;
    z-index: 999;
    background: #000000;
    opacity: 0.7;
}
@keyframes moveWrapper {
    0% {
        transform: translateY(100%);
    }
    100% {
        transform: translateY(0);
    }
}
.wrapper {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 9999;
    padding: 0 30rpx;
    background-color: #fff;
    border-radius: 20rpx 20rpx 0 0;
    animation: moveWrapper .5s;
}
.wrapper .title {
    font-size: 36rpx;
    font-weight: bold;
    color: #333333;
    text-align: center;
    padding: 40rpx 0 29rpx 0;
}
.wrapper .address-list {
    max-height: 648rpx;
    overflow: auto;
}
.address-list::-webkit-scrollbar {
    display:none
}
.wrapper .address-item {
    display: flex;
    padding: 39rpx 0;
    border-bottom: 1rpx dashed #F7F7F7;
}
.address-item  .radio {
    transform-origin: 0 0;
    transform: scale(0.59);
}
.address-item .address-item-info {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    flex: 1;
}
.address-item .address-item-info view:first-child {
    display: flex;
    justify-content: space-between;
    margin-bottom: 29rpx;
    line-height: 1;
}
.btn-box {
    padding: 29rpx 0 40rpx 0;
}
.wrapper .btn {
    background-color: #F5F5F5;
}
.tab-bar-box{
    height: 90rpx;
    background: white;
    margin-bottom: 20rpx;
    font-size: 34rpx;
    font-weight: 500;
    color: #666666;
    box-sizing: border-box;
}
.tab-bar-box view {
    display: inline-block;
    flex: 1;
    height: 90rpx;
    line-height: 90rpx;
    box-sizing: border-box;
}
.tab-bar-box view:first-child {
    margin: 0 160rpx 0 164rpx;
}
.active-tab {
    color: #FE3636;
    border-bottom: 6rpx solid #FE3636;
}
.pick-place-box {
    background-color: #ffffff;
    padding: 0 30rpx;
}
.pick-place-box .name, .pick-place-box .mobile {
    display: flex;
    align-items: center;
    height: 108rpx;
    border-bottom: 1rpx dashed #EEEEEE;
    padding: 39rpx 0;
    box-sizing: border-box;
    font-size: 30rpx;
    font-weight: 500;
    color: #333333;
}
.pick-place-box .mobile {
    border-bottom: none;
}
.pick-place-box .input {
    flex: 1;
    margin-left: 40rpx;
}
.pick-place-box .pick-place {
    padding: 40rpx 0;
    display: flex;
    align-items: center;
    border-bottom: 1rpx dashed #EEEEEE;
}
.pick-place .select-box {
    flex: 1;
}
.pick-place .select-box view:first-child {
    display: flex;
    justify-content: space-between;
    font-size: 34rpx;
    font-weight: bold;
    color: #333333;
}
.pick-place .select-box view:last-child {
    display: flex;
    justify-content: space-between;
    font-size: 30rpx;
    font-weight: 500;
    color: #666666;
    margin-top: 29rpx;
}
.address-distance {
    text-align: right;
}

.goods-list-group {
    background-color: #ffffff;
    margin-bottom: 20rpx;
    padding: 0 30rpx;
}
.goods-list-group .goods-item {
    display: flex;
    padding: 30rpx 0;
}
.goods-list-group .goods-item image {
    width: 180rpx;
    height: 180rpx;
    border-radius: 10rpx;
    margin-right: 31rpx;
}
.goods-list-group .goods-item .goods-item-info {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    flex: 1;
}
.goods-item .goods-item-info .goods-item-title {
    font-size: 32rpx;
    font-weight: 500;
    color: #333333;
}
.goods-item .goods-item-info .goods-item-title .goods-item-standard {
    display: block;
    font-size: 30rpx;
    font-weight: 500;
    color: #999999;
    margin-top: 29rpx;
}
.goods-item-price {
    display: flex;
    justify-content: space-between;
    align-items: baseline;
}
.goods-item-price text:first-child {
    font-size: 36rpx;
    font-weight: bold;
    color: #F6271D;
}
.goods-item-price text:last-child {
    font-size: 30rpx;
    font-weight: 500;
    color: #999999;
}

.other-group {
    background-color: #ffffff;
    padding: 0 30rpx;
}
.other-group .item-box {
    padding: 40rpx 0;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx dashed #EEEEEE;
}
.other-group .item-box text:first-child {
    font-size: 30rpx;
    font-weight: 500;
}
.other-group .item-box .input-placeholder {
    text-align: right;
    font-size: 30rpx;
    font-weight: 500;
    color: #999999;
}
.other-group .item-box .input {
    text-align: right;
}
.other-group .price-info {
    padding: 40rpx 0;
    font-weight: 500;
    text-align: right;
}

