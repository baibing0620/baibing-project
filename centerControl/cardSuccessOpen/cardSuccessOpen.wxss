.head_bg{
    width: 100%;
    height: 503rpx;
    background: url('https://facing-1256908372.file.myqcloud.com//image/20190403/8976c4efad6a8da0.jpg') no-repeat;
    background-size: cover;
    position: relative;
}
.head_bg .head_bg_circle{
    width: 100%;
    height: 503rpx;
    background: url('https://facing-1256908372.file.myqcloud.com//image/20190403/1deb1eaa04c36ba2.png') no-repeat center 61rpx;
    background-size: 208rpx;

}
.head_bg .head_bg_circle .iconfont{
    font-size: 70rpx;
    color: white;
    font-weight: normal;
}
.head_bg .head_bg_circle .head_bg_circle_contain{
    width: 126rpx;
    height: 80rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 32rpx;
    color: white;
    position: relative;
    left: 50%;
    top: 119rpx;
    margin-left: -61rpx;
    font-weight: bold;
    white-space:nowrap;
}
.head_bg .head_bg_notice{
    position: absolute;
    left: 50%;
    margin-left: -178rpx;
    top: 296rpx;
    white-space: nowrap;
    font-size: 26rpx;
    color: #fff;
}

.head_contain{
    width: 100%;
    text-align: center;
    font-size: 34rpx;
    background-color: #fff;
    padding-bottom: 61rpx;
    border-bottom: 12rpx solid #f5f5f5;
}
.head_contain .head_contain_phone{
    font-size: 26rpx;
    color: #1a1a1a;
}
.head_contain button{
    width: 470rpx;
    height: 80rpx;
    margin-top: 30rpx;
    background-color: #ff9b1f;
    font-weight: bold;
}
.head_contain .button-hover{
    background-color: rgba(255, 155, 31, 0.9);
    color: #fff;
}



.contant{
    padding: 43rpx 30rpx 32rpx 30rpx;
    font-size: 28rpx;
    color: #666666;
    box-sizing: border-box;
    background-color: #fff;
}
.openDetails{
  padding-bottom: 32rpx;
  box-sizing: border-box;
}
.openDetails_title{
    font-size: 30rpx;
    color: #1a1a1a;
    margin-bottom: 27rpx;
    font-weight: bold;
}
.openDetails_title~view{
    line-height: 56rpx;
}
.openDetails .money{
    display: inline;
    color: #ff4c4c;
}
.openDetails_2{
