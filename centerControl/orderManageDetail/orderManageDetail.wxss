.goods-list-group {
    background-color: #ffffff;
    margin-bottom: 20rpx;
    padding: 0 30rpx;
}
.goods-list-group .goods-item {
    display: flex;
    padding: 30rpx 0;
}
.goods-list-group .goods-item image {
    width: 180rpx;
    height: 180rpx;
    border-radius: 10rpx;
    margin-right: 31rpx;
}
.goods-list-group .goods-item .goods-item-info {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    flex: 1;
}
.goods-item .goods-item-info .goods-item-title {
    font-size: 32rpx;
    font-weight: 500;
    color: #333333;
}
.goods-item .goods-item-info .goods-item-title .goods-item-standard {
    display: block;
    font-size: 30rpx;
    font-weight: 500;
    color: #999999;
    margin-top: 29rpx;
}
.goods-item-price {
    display: flex;
    justify-content: space-between;
    align-items: baseline;
}
.goods-item-price text:first-child {
    font-size: 36rpx;
    font-weight: bold;
    color: #F6271D;
}
.goods-item-price text:last-child {
    font-size: 30rpx;
    font-weight: 500;
    color: #999999;
}


